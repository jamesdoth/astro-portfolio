---
const textLinks: { label: string; href: string }[] = [
    { label: "Home", href: "/" },
    { label: "Content", href: "/content/" },
    { label: "About", href: "/about/" },
];
---

<nav class="absolute h-[10%] w-screen flex justify-between items-center z-50">
    <div class="ml-12">
        <a href="/" class="site-title text-xl text-white"> James Young</a>
    </div>
    <div class="mr-12">
        <button
            class="hamburger bg-gray-800 p-2 rounded-md text-gray-400 hover:text-white hover:bg-gray-700 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-offset-gray-800 focus:ring-white"
            aria-expanded="false"
        >
            <span class="sr-only">Menu</span>
            <svg
                class="block h-6 w-6"
                xmlns="http://www.w3.org/2000/svg"
                fill="none"
                viewBox="0 0 24 24"
                stroke="currentColor"
                aria-hidden="true"
            >
                <path
                    stroke-linecap="round"
                    stroke-linejoin="round"
                    stroke-width="2"
                    d="M4 6h16M4 12h16M4 18h16"></path>
            </svg>
        </button>
    </div>
</nav>
